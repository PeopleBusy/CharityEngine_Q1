            <td>
                @Html.DisplayFor(model => item.OwnerUnitNumber)
            </td>
            <td>
                @Html.DisplayFor(model => item.OwnerAppartementNumber)
            </td>
            <td>
                @Html.DisplayFor(model => item.VehiceMake)
            </td>
            <td>
                @Html.DisplayFor(model => item.VehiceModel)
            </td>
            <td>
                @Html.DisplayFor(model => item.VehiceColor)
            </td>
            <td>
                @Html.DisplayFor(model => item.RegistrationDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.VehiceRegistrationNumber }))
            </td>
        </tr>
    }

</table>