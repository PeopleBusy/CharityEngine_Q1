@model IEnumerable<CharityEngine_Q1.Models.VehicleModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <td>
            @Html.DisplayNameFor(model => model.VehiceRegistrationNumber)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.OwnerName)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.OwnerPhoneNumber)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.OwnerUnitNumber)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.OwnerAppartementNumber)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.VehiceMake)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.VehiceModel)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.VehiceColor)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.RegistrationDate)
        </td>
        <td></td>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.VehiceRegistrationNumber)
